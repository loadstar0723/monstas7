{
  "name": "frontend",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "predev": "node scripts/smart-validator.js",
    "dev": "next dev -H 0.0.0.0 -p 3000",
    "dev:clean": "rmdir /s /q .next 2>nul && next dev -H 0.0.0.0 -p 3000",
    "validate": "node scripts/smart-validator.js",
    "validate:strict": "node scripts/enforce-no-fake-data.js || echo Warning: Found violations but continuing...",
    "validate:watch": "node scripts/watch-validate.js",
    "validate:fix": "node scripts/auto-fix-violations.js",
    "validate:report": "node scripts/enforce-no-fake-data.js > validation-report.txt 2>&1 || echo Report saved",
    "validate:mode:off": "echo {\"mode\":\"off\"} > scripts/.validation-config.json",
    "validate:mode:warning": "echo {\"mode\":\"warning\"} > scripts/.validation-config.json",
    "validate:mode:strict": "echo {\"mode\":\"strict\"} > scripts/.validation-config.json",
    "prebuild": "node scripts/smart-validator.js",
    "build": "next build",
    "build:clean": "rm -rf .next && next build",
    "start": "next start -H 0.0.0.0 -p 3000",
    "clean": "rmdir /s /q .next 2>nul",
    "clean:linux": "rm -rf .next",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "pre-deploy": "npm run fix:prisma && npm run build",
    "fix:prisma": "npx prisma generate",
    "detect:mock": "node scripts/detect-mock-data.js",
    "check:random": "grep -r \"Math.random\" --include=\"*.tsx\" --include=\"*.ts\" --exclude-dir=node_modules --exclude-dir=.next . | wc -l || echo 0"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.15.0",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.86.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/nodemailer": "^7.0.1",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "chart.js": "^4.5.0",
    "clsx": "^2.1.1",
    "critters": "^0.0.23",
    "framer-motion": "^12.23.12",
    "i18next": "^25.5.0",
    "jose": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "lightweight-charts": "^5.0.8",
    "lucide-react": "^0.543.0",
    "next": "15.5.2",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.10.1",
    "prisma": "^6.15.0",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-i18next": "^15.7.3",
    "react-icons": "^5.5.0",
    "recharts": "^3.2.0",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "sharp": "^0.34.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
